{
    "name": "lotgd/module-res-form",
    "description": "Resource module to support forms in attachments.",
    "type": "lotgd-module",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Basilius Sauter",
            "email": "basilius.sauter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LotGD\\Module\\Res\\Form\\": "src/",
            "LotGD\\Module\\Res\\Form\\Tests\\": "tests/"
        }
